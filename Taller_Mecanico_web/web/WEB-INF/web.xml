<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>CrearModificarCliente</servlet-name>
        <servlet-class>Servlets.CrearModificarCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearModificarAdministrador</servlet-name>
        <servlet-class>Servlets.CrearModificarAdministrador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearFacturaCompra</servlet-name>
        <servlet-class>Servlets.CrearFacturaCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearDetalleCompra</servlet-name>
        <servlet-class>Servlets.CrearDetalleCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarFacturaCompra</servlet-name>
        <servlet-class>Servlets.ActualizarFacturaCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearFacturaVenta</servlet-name>
        <servlet-class>Servlets.CrearFacturaVenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearFacturaVenta_V2</servlet-name>
        <servlet-class>Servlets.CrearFacturaVenta_V2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearDetalleVenta</servlet-name>
        <servlet-class>Servlets.CrearDetalleVenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearDetalleVentaSer</servlet-name>
        <servlet-class>Servlets.CrearDetalleVentaSer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarFacturaVenta</servlet-name>
        <servlet-class>Servlets.ActualizarFacturaVenta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CrearModificarCliente</servlet-name>
        <url-pattern>/CrearModificarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearModificarAdministrador</servlet-name>
        <url-pattern>/CrearModificarAdministrador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearFacturaCompra</servlet-name>
        <url-pattern>/CrearFacturaCompra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearDetalleCompra</servlet-name>
        <url-pattern>/CrearDetalleCompra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarFacturaCompra</servlet-name>
        <url-pattern>/ActualizarFacturaCompra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearFacturaVenta</servlet-name>
        <url-pattern>/CrearFacturaVenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearFacturaVenta_V2</servlet-name>
        <url-pattern>/CrearFacturaVenta_V2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearDetalleVenta</servlet-name>
        <url-pattern>/CrearDetalleVenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearDetalleVentaSer</servlet-name>
        <url-pattern>/CrearDetalleVentaSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarFacturaVenta</servlet-name>
        <url-pattern>/ActualizarFacturaVenta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
